<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingRight="30dp">

    <LinearLayout
        android:id="@id/ll_middle_answer_result_item_muti_user_answer_info"
        android:layout_width="wrap_content"
        android:layout_height="23dp"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:id="@id/iv_middle_answer_result_item_muti_right_answer"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="5dp" />

        <TextView
            android:id="@id/tv_middle_answer_result_item_muti_index"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/iv_middle_answer_result_item_muti_right_answer"
            android:text=""
            android:minWidth="15dp"
            android:textColor="#726665"
            android:textSize="17sp" />
    </LinearLayout>

    <!--选择题 标准答案-->
    <TextView
        android:id="@id/tv_middle_answer_result_item_muti_user_answer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_toRightOf="@id/ll_middle_answer_result_item_muti_user_answer_info"
        android:text="11"
        android:textColor="@color/COLOR_333333"
        android:textSize="17sp"
        android:visibility="visible" />


    <TextView
        android:layout_below="@id/tv_middle_answer_result_item_muti_user_answer"
        android:id="@id/tv_middle_answer_result_item_muti_right_answer"
        android:layout_width="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="3dp"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/ll_middle_answer_result_item_muti_user_answer_info"
        android:text=""
        android:textColor="@color/COLOR_333333"
        android:textSize="17sp"
        android:visibility="visible" />
    <View
        android:layout_below="@id/tv_middle_answer_result_item_muti_right_answer"
        android:layout_width="match_parent"
        android:layout_height="10dp"/>

</RelativeLayout>