<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingRight="30dp">

    <LinearLayout
        android:id="@id/ll_arts_answer_result_item_muti_user_answer_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:id="@id/iv_arts_answer_result_item_muti_icon"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="11dp" />

        <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
            android:id="@id/tv_arts_answer_result_item_muti_index"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textColor="#726665"
            android:textSize="16sp" />

    </LinearLayout>


    <!--选择题 标准答案-->
    <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
        android:layout_toRightOf="@id/ll_arts_answer_result_item_muti_user_answer_info"
        android:id="@id/tv_arts_answer_result_item_muti_user_answer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="#726665"
        android:textSize="16sp" />

    <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
        android:id="@id/tv_arts_answer_result_item_muti_right_answer"
        android:layout_width="wrap_content"
        android:layout_below="@id/tv_arts_answer_result_item_muti_user_answer"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/ll_arts_answer_result_item_muti_user_answer_info"
        android:text=""
        android:textColor="#726665"
        android:textSize="16sp"
        android:visibility="visible" />


</RelativeLayout>