<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
        android:id="@+id/tv_livevideo_switch_flow_route1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="线路一" />

    <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
        android:id="@+id/tv_livevideo_switch_flow_route2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="线路二"
        app:layout_constraintTop_toBottomOf="@+id/tv_livevideo_switch_flow_route1" />

    <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
        android:id="@+id/tv_livevideo_switch_flow_route3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="线路三"
        app:layout_constraintTop_toBottomOf="@+id/tv_livevideo_switch_flow_route2" />

    <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
        android:id="@+id/tv_livevideo_switch_flow_route4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="线路四"
        app:layout_constraintTop_toBottomOf="@+id/tv_livevideo_switch_flow_route3" />

</android.support.constraint.ConstraintLayout>