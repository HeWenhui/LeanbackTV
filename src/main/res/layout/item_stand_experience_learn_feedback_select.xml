<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
        android:id="@+id/fzcytv_stand_experience_learn_feedback_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="11sp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageView
        android:id="@+id/iv_stand_experience_learn_feedback_select_button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:src="@drawable/selector_stand_experience_learn_feedback_button"
        app:layout_constraintTop_toBottomOf="@+id/fzcytv_stand_experience_learn_feedback_title" />

    <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
        android:id="@+id/fzcytv_stand_experience_learn_feedback_select1_button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:textSize="10sp"
        app:layout_constraintLeft_toRightOf="@+id/iv_stand_experience_learn_feedback_select_button1" />

    <ImageView
        android:id="@+id/iv_stand_experience_learn_feedback_select_button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/selector_stand_experience_learn_feedback_button"
        app:layout_constraintHorizontal_bias="0.35"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/iv_stand_experience_learn_feedback_select_button1" />

    <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
        android:id="@+id/fzcytv_stand_experience_learn_feedback_select1_button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/iv_stand_experience_learn_feedback_select_button2"
        app:layout_constraintLeft_toRightOf="@+id/iv_stand_experience_learn_feedback_select_button2"
        app:layout_constraintTop_toTopOf="@+id/iv_stand_experience_learn_feedback_select_button2" />

    <android.support.constraint.Group
        android:id="@+id/group_stand_experience_learn_feedback_select"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="iv_stand_experience_learn_feedback_select_button3,fzcytv_stand_experience_learn_feedback_select1_button3" />

    <ImageView
        android:id="@+id/iv_stand_experience_learn_feedback_select_button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/selector_stand_experience_learn_feedback_button"
        app:layout_constraintHorizontal_bias="0.7"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/iv_stand_experience_learn_feedback_select_button1" />

    <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
        android:id="@+id/fzcytv_stand_experience_learn_feedback_select1_button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/iv_stand_experience_learn_feedback_select_button3"
        app:layout_constraintLeft_toRightOf="@+id/iv_stand_experience_learn_feedback_select_button3"
        app:layout_constraintTop_toTopOf="@+id/iv_stand_experience_learn_feedback_select_button3" />
</android.support.constraint.ConstraintLayout>