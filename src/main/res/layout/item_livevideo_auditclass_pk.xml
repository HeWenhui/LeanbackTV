<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#33333B">

    <android.support.constraint.ConstraintLayout
        android:id="@id/consl_livevideo_auditclass_test_tilte_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:background="#2A2A33"
        app:layout_constraintTop_toBottomOf="@id/tv_livevideo_auditclass_against_energy_anchor">

        <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
            android:id="@id/tv_livevideo_auditclass_rate_anchor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="5dp"
            android:text="我的正确率"
            android:textColor="@color/white"
            android:textSize="10sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
            android:id="@id/tv_livevideo_auditclass_rate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="9dp"
            android:textColor="@color/white"
            android:textSize="10sp"
            app:layout_constraintBottom_toBottomOf="@id/tv_livevideo_auditclass_rate_anchor"
            app:layout_constraintRight_toRightOf="parent" />


        <View
            android:id="@id/v_livevideo_auditclass_devide_line"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="9dp"
            android:layout_marginRight="9dp"
            android:layout_marginTop="5dp"
            android:background="#000000"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_livevideo_auditclass_rate_anchor" />



            <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
                android:id="@id/tv_livevideo_auditclass_title_test_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="9dp"
                android:minWidth="20dp"
                android:paddingBottom="5dp"
                android:text="题号"
                android:textColor="@color/white"
                android:textSize="10sp"
                android:layout_marginTop="12dp"
                app:layout_constraintTop_toBottomOf="@id/tv_livevideo_auditclass_rate_anchor"
                app:layout_constraintLeft_toLeftOf="parent" />


            <android.support.constraint.Guideline
                android:id="@id/gl_live_auditclass_tile_anchor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.293" />

            <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
                android:id="@id/tv_livevideo_auditclass_title_answer_result"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:minWidth="40dp"
                android:paddingBottom="5dp"
                android:text="作答结果"
                android:textColor="@color/white"
                android:textSize="10sp"
                app:layout_constraintBottom_toBottomOf="@id/tv_livevideo_auditclass_title_test_num"
                app:layout_constraintLeft_toRightOf="@id/gl_live_auditclass_tile_anchor" />

            <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
                android:id="@id/tv_livevideo_auditclass_title_rate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="9dp"
                android:layout_marginTop="12dp"
                android:minWidth="50dp"
                android:paddingBottom="5dp"
                android:text="全场正确率"
                android:textColor="@color/white"
                android:textSize="10sp"
                app:layout_constraintBottom_toBottomOf="@id/tv_livevideo_auditclass_title_test_num"
                app:layout_constraintRight_toRightOf="parent" />

    </android.support.constraint.ConstraintLayout>


</android.support.constraint.ConstraintLayout>