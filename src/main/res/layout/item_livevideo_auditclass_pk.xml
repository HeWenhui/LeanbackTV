<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="120dp"
    android:paddingLeft="5dp"
    android:paddingRight="5dp"
    >

    <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
        android:id="@id/tv_livevideo_auditclass_team_rank_anchor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="战队排名"
        android:textSize="14sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:text="228"
        android:textSize="14sp"
        android:id="@id/tv_livevideo_auditclass_myrank"
        app:layout_constraintLeft_toRightOf="@id/tv_livevideo_auditclass_team_rank_anchor"
        app:layout_constraintTop_toTopOf="@id/tv_livevideo_auditclass_team_rank_anchor" />

    <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
        android:id="@id/tv_livevideo_auditclass_energy_anchor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="本队"
        android:textSize="14sp"
        app:layout_constraintLeft_toLeftOf="@id/tv_livevideo_auditclass_team_rank_anchor"
        app:layout_constraintTop_toBottomOf="@id/tv_livevideo_auditclass_team_rank_anchor" />

    <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
        android:id="@id/tv_livevideo_auditclass_energy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="3dp"
        android:drawablePadding="3dp"
        android:drawableRight="@drawable/livevideo_psenergyname_img_disable"
        android:text="899"
        android:textSize="14sp"
        app:layout_constraintLeft_toRightOf="@id/tv_livevideo_auditclass_energy_anchor"
        app:layout_constraintTop_toTopOf="@id/tv_livevideo_auditclass_energy_anchor" />

    <!--  -->

    <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
        android:id="@id/tv_livevideo_auditclass_against_energy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawablePadding="3dp"
        android:drawableRight="@drawable/livevideo_psenergyname_img_disable"
        android:text="268"
        android:textSize="14sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/tv_livevideo_auditclass_energy_anchor" />

    <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
        android:id="@id/tv_livevideo_auditclass_against_energy_anchor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="3dp"
        android:text="对手"
        android:textSize="14sp"
        app:layout_constraintRight_toLeftOf="@id/tv_livevideo_auditclass_against_energy"
        app:layout_constraintTop_toTopOf="@id/tv_livevideo_auditclass_against_energy" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/half_alpha_black"
        app:layout_constraintTop_toBottomOf="@id/tv_livevideo_auditclass_against_energy_anchor"
        android:layout_marginTop="10dp"
        android:id="@id/v_livevideo_auditclass_devide_line"
        />

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/v_livevideo_auditclass_devide_line"
        android:id="@id/consl_livevideo_auditclass_test_tilte_container"
        >
        <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的正确率"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="10dp"
            android:textSize="14sp"
            android:id="@id/tv_livevideo_auditclass_rate_anchor"
            />
        <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="50%"
            android:textSize="14sp"
            app:layout_constraintLeft_toRightOf="@id/tv_livevideo_auditclass_rate_anchor"
            app:layout_constraintBottom_toBottomOf="@id/tv_livevideo_auditclass_rate_anchor"
            android:layout_marginLeft="3dp"
            android:id="@id/tv_livevideo_auditclass_rate"
            />


        <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="题号"
            android:textSize="11sp"
            android:id="@id/tv_livevideo_auditclass_title_test_num"
            app:layout_constraintTop_toBottomOf="@id/tv_livevideo_auditclass_rate_anchor"
            android:layout_marginTop="10dp"
            app:layout_constraintWidth_percent=".2"
            />

        <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="作答结果"
            android:textSize="11sp"
            android:id="@id/tv_livevideo_auditclass_title_answer_result"
            app:layout_constraintLeft_toRightOf="@id/tv_livevideo_auditclass_title_test_num"
            app:layout_constraintBottom_toBottomOf="@id/tv_livevideo_auditclass_title_test_num"
            android:layout_marginTop="10dp"
            app:layout_constraintWidth_percent=".4"
            />

        <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="11sp"
            android:text="全场正确率"
            android:id="@id/tv_livevideo_auditclass_title_rate"
            app:layout_constraintLeft_toRightOf="@id/tv_livevideo_auditclass_title_answer_result"
            app:layout_constraintBottom_toBottomOf="@id/tv_livevideo_auditclass_title_test_num"
            android:layout_marginTop="10dp"
            app:layout_constraintWidth_percent=".4"
            />


    </android.support.constraint.ConstraintLayout>

   <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="暂未数据"
       android:textSize="14sp"
       android:id="@id/tv_livevideo_auditclass_no_testinfo"
       app:layout_constraintTop_toBottomOf="@id/v_livevideo_auditclass_devide_line"
       android:layout_marginTop="10dp"
       android:visibility="gone"
       />

</android.support.constraint.ConstraintLayout>