<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="MissingConstraints">

    <TextView
        android:id="@id/tv_livevideo_rank_item_left"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:gravity="center"
        android:textSize="11dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="fadfads" />

    <TextView
        android:id="@id/tv_livevideo_rank_item_mid_left"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="7dp"
        android:ellipsize="end"
        app:layout_constraintWidth_percent=".27"
        android:lines="1"
        android:textSize="11dp"
        app:layout_constraintBottom_toBottomOf="@id/tv_livevideo_rank_item_left"
        app:layout_constraintLeft_toRightOf="@id/tv_livevideo_rank_item_left"
        app:layout_constraintTop_toTopOf="@id/tv_livevideo_rank_item_left"
        tools:text="fadfads" />

    <TextView
        android:id="@id/tv_livevideo_rank_item_mid_right"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="4dp"
        android:gravity="left"
        android:textSize="11dp"
        app:layout_constraintBottom_toBottomOf="@id/tv_livevideo_rank_item_left"
        app:layout_constraintRight_toLeftOf="@id/iv_livevideo_rank_item_right_leftimg"
        app:layout_constraintTop_toTopOf="@id/tv_livevideo_rank_item_left"
        tools:text="1000" />

    <ImageView
        android:id="@id/iv_livevideo_rank_item_right_leftimg"
        android:layout_width="22dp"
        android:layout_height="17dp"
        android:layout_marginTop="7dp"
        android:layout_marginRight="18dp"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:paddingBottom="5dp"
        android:src="@drawable/livevideo_list_heart_icon_normal"
        app:layout_constraintBottom_toBottomOf="@id/tv_livevideo_rank_item_left"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/tv_livevideo_rank_item_left" />

    <TextView
        android:id="@id/tv_livevideo_rank_item_right"
        android:layout_width="25dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="3dp"
        android:textSize="11dp"
        app:layout_constraintBottom_toBottomOf="@id/tv_livevideo_rank_item_left"
        app:layout_constraintLeft_toRightOf="@id/iv_livevideo_rank_item_right_leftimg"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/tv_livevideo_rank_item_left"
        tools:text="123" />
</android.support.constraint.ConstraintLayout>