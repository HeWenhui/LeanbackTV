<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@id/rl_livevideo_en_achive_layour"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:id="@id/rl_livevideo_en_achive_back"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/app_livevideo_enteampk_benchangchengjiu_bg_img_nor">

        <RelativeLayout
            android:id="@id/rl_livevideo_en_achive_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true">

            <ImageView
                android:id="@id/iv_livevideo_en_achive_title"
                android:layout_width="48dp"
                android:layout_height="12dp"
                android:layout_marginTop="7dp"
                android:src="@drawable/app_livevideo_enteampk_benchangchengjiu_title_pic_nor" />

            <CheckBox
                android:id="@id/cb_livevideo_en_achive_title"
                android:layout_width="17dp"
                android:layout_height="17dp"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/iv_livevideo_en_achive_title"
                android:background="@drawable/selector_live_en_achive_open"
                android:button="@null"
                android:checked="false" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@id/rl_livevideo_en_achive_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/rl_livevideo_en_achive_title"
            android:layout_marginTop="9dp"
            android:visibility="gone">

            <RelativeLayout
                android:id="@id/rl_livevideo_en_achive_content_num"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal">

                <ImageView
                    android:id="@id/iv_livevideo_en_achive_num_fire"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/livevideo_enteampk_benchangchengjiu_fair_img_nor" />

                <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
                    android:id="@id/tv_livevideo_en_achive_num_fire"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@id/iv_livevideo_en_achive_num_star"
                    android:layout_alignBottom="@id/iv_livevideo_en_achive_num_star"
                    android:layout_toRightOf="@id/iv_livevideo_en_achive_num_fire"
                    android:gravity="center_vertical"
                    android:includeFontPadding="false"
                    android:text="0"
                    android:textColor="@color/COLOR_FF449CEE"
                    android:textSize="12sp" />

                <ImageView
                    android:id="@id/iv_livevideo_en_achive_num_star"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="9dp"
                    android:layout_toRightOf="@id/tv_livevideo_en_achive_num_fire"
                    android:src="@drawable/livevideo_enteampk_benchangchengjiu_star_img_nor" />

                <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
                    android:id="@id/tv_livevideo_en_achive_num_star"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@id/iv_livevideo_en_achive_num_star"
                    android:layout_alignBottom="@id/iv_livevideo_en_achive_num_star"
                    android:layout_toRightOf="@id/iv_livevideo_en_achive_num_star"
                    android:gravity="center_vertical"
                    android:includeFontPadding="false"
                    android:text="0"
                    android:textColor="@color/COLOR_FF449CEE"
                    android:textSize="12sp" />

                <ImageView
                    android:id="@id/iv_livevideo_en_achive_num_gold"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@id/tv_livevideo_en_achive_num_gold"
                    android:src="@drawable/livevideo_enteampk_benchangchengjiu_gold_img_nor" />

                <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
                    android:id="@id/tv_livevideo_en_achive_num_gold"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@id/iv_livevideo_en_achive_num_star"
                    android:layout_alignBottom="@id/iv_livevideo_en_achive_num_star"
                    android:layout_marginRight="7dp"
                    android:layout_toLeftOf="@id/iv_livevideo_en_achive_num_fire"
                    android:gravity="center_vertical"
                    android:includeFontPadding="false"
                    android:text="0"
                    android:textColor="@color/COLOR_FF449CEE"
                    android:textSize="12sp" />

            </RelativeLayout>

            <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
                android:id="@+id/tv_livevideo_en_achive_aim_empty"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/rl_livevideo_en_achive_content_num"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="17dp"
                android:text="@string/livevideo_enpk_notarget"
                android:textColor="@color/COLOR_FF449CEE"
                android:textSize="9sp" />

            <RelativeLayout
                android:id="@+id/rl_livevideo_en_achive_aim_content"
                android:layout_width="165dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/rl_livevideo_en_achive_content_num"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="8dp"
                android:visibility="gone">

                <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
                    android:id="@+id/tv_livevideo_en_achive_aimtype"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/pg_livevideo_en_achive_aim"
                    android:layout_marginLeft="3dp"
                    android:text="正确率"
                    android:textColor="@color/COLOR_FF449CEE"
                    android:textSize="9dp" />

                <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
                    android:id="@+id/tv_livevideo_en_achive_aimvalue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignRight="@+id/pg_livevideo_en_achive_aim"
                    android:layout_marginRight="3dp"
                    android:text="目标80%"
                    android:textColor="@color/COLOR_FF449CEE"
                    android:textSize="9dp" />

                <ProgressBar
                    android:id="@+id/pg_livevideo_en_achive_aim"
                    style="@style/ProgressBar_live_achive_betterme"
                    android:layout_width="143dp"
                    android:layout_height="13dp"
                    android:layout_below="@+id/tv_livevideo_en_achive_aimtype"
                    android:layout_centerHorizontal="true"
                    android:max="100"
                    android:paddingLeft="0dp"
                    android:paddingRight="0dp"
                    android:progress="50" />

                <com.xueersi.parentsmeeting.widget.FangZhengCuYuanTextView
                    android:id="@+id/tv_livevideo_en_achive_aimtips"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/pg_livevideo_en_achive_aim"
                    android:background="@drawable/livevideo_enteampk_xiaomubiao_tips_bg_nor"
                    android:gravity="center"
                    android:paddingTop="3dp"
                    android:layout_alignParentLeft="true"
                    android:textColor="@color/white"
                    android:textSize="9dp" />

            </RelativeLayout>

        </RelativeLayout>

    </RelativeLayout>

    <ViewStub
        android:id="@id/vs_livevideo_en_achive_bottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/rl_livevideo_en_achive_back"
        android:layout="@layout/layout_livevodeo_en_achive_pk" />

    <ViewStub
        android:id="@id/vs_livevideo_en_achive_bottom2"
        android:layout_width="match_parent"
        android:layout_height="28dp"
        android:layout_below="@id/rl_livevideo_en_achive_back"
        android:layout="@layout/layout_livevodeo_en_achive_nopk" />

</RelativeLayout>