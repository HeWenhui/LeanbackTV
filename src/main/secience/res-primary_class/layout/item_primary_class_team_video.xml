<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="96dp"
    android:layout_height="72dp"
    tools:ignore="MissingConstraints,NotSibling">

    <android.support.constraint.ConstraintLayout
        android:layout_width="35dp"
        android:layout_height="15dp"
        android:layout_marginTop="3dp"
        android:layout_marginRight="2dp"
        android:background="@drawable/shape_primary_item_eneygy_back"
        app:layout_constraintRight_toRightOf="parent">

        <ImageView
            android:id="@id/iv_livevideo_primary_team_energy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:src="@drawable/toast_energe2_icon_normal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@id/tv_livevideo_primary_team_energy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="6dp"
            android:gravity="center"
            android:includeFontPadding="false"
            android:text="10"
            android:textColor="@color/white"
            android:textSize="9sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toRightOf="@id/iv_livevideo_primary_team_energy"
            app:layout_constraintTop_toTopOf="parent" />
    </android.support.constraint.ConstraintLayout>

    <TextView
        android:id="@id/tv_livevideo_primary_team_people_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginBottom="3dp"
        android:shadowColor="#1a000000"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="3.0"
        android:text="小铝孩"
        android:textColor="#ffffffff"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent" />

    <ImageView
        android:id="@id/iv_livevideo_primary_team_voice_open"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/xuesheng_icon_maikefeng_normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <com.xueersi.parentsmeeting.modules.livevideo.primaryclass.weight.VoiceImageView
        android:id="@id/iv_livevideo_primary_team_voice_voice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/bg_live_voicewave_bg2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintBottom_toTopOf="@id/iv_livevideo_primary_team_voice_open"
        app:layout_constraintLeft_toLeftOf="@id/iv_livevideo_primary_team_voice_open"
        app:layout_constraintRight_toRightOf="@id/iv_livevideo_primary_team_voice_open" />

    <ImageView
        android:id="@id/iv_livevideo_primary_team_energy_add"
        android:layout_width="54dp"
        android:layout_height="22dp"
        android:layout_marginLeft="5dp"
        android:layout_marginBottom="4dp"
        android:background="@drawable/shape_primary_item_eneygy_add_back"
        android:src="@drawable/toast_energe1_icon_normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>