        <TextView
            android:id="@id/iv_livevideo_en_teampk_lead_fire_add_left"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="3dp"
            android:layout_toRightOf="@id/iv_livevideo_en_teampk_lead_fire_left"
            android:text="+10"
            android:textColor="#ffffdf95"
            android:textSize="15sp" />

        <TextView
            android:id="@id/tv_livevideo_en_teampk_lead_score_left"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/iv_livevideo_en_teampk_lead_fire_left"
            android:text="100"
            android:textColor="#ffffffff"
            android:textSize="21sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@id/rl_livevideo_en_teampk_lead_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/pg_livevideo_en_teampk_lead"
        android:layout_marginLeft="4dp"
        android:layout_toRightOf="@id/pg_livevideo_en_teampk_lead">

        <ImageView
            android:id="@id/iv_livevideo_en_teampk_lead_fire_right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/zhanduipk_kezhongdati_huomiao_pic_nor" />

        <TextView
            android:id="@id/iv_livevideo_en_teampk_lead_fire_add_right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="3dp"
            android:layout_toRightOf="@id/iv_livevideo_en_teampk_lead_fire_right"
            android:text="+10"
            android:textColor="#ffffdf95"
            android:textSize="15sp" />

        <TextView
            android:id="@id/tv_livevideo_en_teampk_lead_score_right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/iv_livevideo_en_teampk_lead_fire_right"
            android:text="100"
            android:textColor="#ffffffff"
            android:textSize="21sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@id/rl_livevideo_en_teampk_lead_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/pg_livevideo_en_teampk_lead"
        android:clipChildren="false">

    </RelativeLayout>

</RelativeLayout>