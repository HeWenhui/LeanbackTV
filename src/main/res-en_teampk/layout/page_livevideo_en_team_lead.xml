        android:src="@drawable/bg_livevideo_en_teampk_lead_mid" />

    <ImageView
        android:id="@id/iv_livevideo_en_teampk_mine"
        android:layout_width="110dp"
        android:layout_height="127dp"
        android:layout_marginRight="17dp"
        android:layout_marginTop="42dp"
        android:layout_toLeftOf="@id/iv_livevideo_en_teampk_vs"
        android:src="@drawable/livevideo_enteampk_benchangzhanduifenzu_01sweetheartkate_pic_normal" />

    <ImageView
        android:id="@id/iv_livevideo_en_teampk_vs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="88dp"
