        android:src="@drawable/livevideo_enteampk_benchangzhanduifenzu_02rockstarjohn_pic_normal" />

    <ProgressBar
        android:id="@id/pg_livevideo_en_teampk_lead"
        style="@style/ProgressBar_live_enspeek"
        android:layout_width="364dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="191dp"
        android:max="100"
        android:progress="30" />

    <RelativeLayout
        android:id="@id/rl_livevideo_en_teampk_lead_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/pg_livevideo_en_teampk_lead"
        android:layout_marginRight="4dp"
        android:layout_toLeftOf="@id/pg_livevideo_en_teampk_lead">

        <ImageView
            android:id="@id/iv_livevideo_en_teampk_lead_fire_left"
            android:layout_width="wrap_content"
