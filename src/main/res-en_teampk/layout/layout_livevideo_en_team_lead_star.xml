<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@id/iv_livevideo_en_teampk_lead_star"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="13dp"
        android:src="@drawable/livevideo_enpk_gongxianzhixing_pic" />

    <GridView
        android:id="@id/gv_livevideo_en_teampk_lead_star"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_centerHorizontal="true"
        android:clickable="false"
        android:horizontalSpacing="20dp"
        android:listSelector="@color/transparent"
        android:numColumns="6" />

    <TextView
        android:id="@id/tv_livevideo_en_teampk_rank_start_close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="5dp"
        android:textColor="@color/white"
        android:textSize="10sp" />

</RelativeLayout>